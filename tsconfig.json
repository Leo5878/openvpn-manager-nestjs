{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2022",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "strictBindCallApply": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "paths": {
      "@openvpn-manager/*": [
        "src/openvpn-mngr-ts/openvpn-manager/src/*"
      ],
      "@app/openvpn-mngr-ts": [
        "src/openvpn-mngr-ts"
      ],
      "@app/openvpn-mngr-ts/openvpn-manager/*": [
        "src/openvpn-mngr-ts/openvpn-manager/src/*"
      ],
      "@app/openvpn-mngr-ts/openvpn-manager-nestjs/*": [
        "src/openvpn-mngr-ts/openvpn-manager-nestjs/src/*"
      ],
      "@app/openvpn-mngr-ts/*": [
        "src/openvpn-mngr-ts/*"
      ]
    }
  }
}